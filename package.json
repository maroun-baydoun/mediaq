{
  "name": "gh-page-template",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve --config ./webpack.dev.js",
    "format": "prettier --write 'src' 'webpack.*.js'",
    "build": "webpack --config ./webpack.prod.js",
    "generate-image": "node ./generate-image.js",
    "publish": "npm run build && npm run generate-image && node ./publish",
    "pull-template": "git pull https://github.com/maroun-baydoun/gh-page-template.git  master --allow-unrelated-histories"
  },
  "author": {
    "name": "Maroun Baydoun",
    "url": "https://maroun-baydoun.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "babel-loader": "8.2.2",
    "babel-plugin-prismjs": "2.0.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "9.0.0",
    "core-js": "3.14.0",
    "css-loader": "5.2.6",
    "file-loader": "6.2.0",
    "gh-pages": "3.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "mini-css-extract-plugin": "1.6.0",
    "node-html-to-image": "3.1.0",
    "node-sass": "6.0.0",
    "prettier": "2.3.1",
    "sass-loader": "12.0.0",
    "style-loader": "2.0.0",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "@fontsource/cutive-mono": "4.4.5",
    "@fontsource/roboto": "4.4.5",
    "mediaq": "2.0.0",
    "prismjs": "1.23.0"
  },
  "features": {
    "react": false,
    "prismjs": {
      "languages": [
        "shell",
        "js"
      ]
    }
  }
}
